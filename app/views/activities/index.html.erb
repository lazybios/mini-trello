<div class="wrapper clearfix">
    <div class="main clearfix">
        <% @activities.each do |activity| %>
            <%= div_for activity do %>
                <%= link_to activity.user.username %>
                <%= render "activities/#{activity.trackable_type.underscore}/#{activity.action}", activity: activity %>
            <% end %>
        <% end %>
    </div>
</div>
